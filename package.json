{
  "name": "agnet",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "A network for your AI agents",
  "repository": {
    "type": "git",
    "url": "https://github.com/holiber/Agnet"
  },
  "bugs": {
    "url": "https://github.com/holiber/Agnet/issues"
  },
  "homepage": "https://github.com/holiber/Agnet#readme",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "agnet": "./bin/agnet.mjs",
    "mock-agent": "./bin/mock-agent.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsup",
    "tests": "npm run test:unit && npm run test:e2e",
    "test": "npm run test:unit",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "npm run build && playwright test",
    "lint": "tsc -p tsconfig.json --noEmit",
    "docs:api": "tsx scripts/generate-api-docs.ts",
    "docs:api:check": "npm run docs:api && git diff --exit-code -- docs/generated"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.3",
    "openai": "^6.15.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "yaml": "^2.8.2",
    "zod": "^4.3.5"
  }
}
